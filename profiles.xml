<?xml version="1.0" encoding="UTF-8"?>
<profilesXml xmlns="http://maven.apache.org/PROFILES/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/PROFILES/1.0.0 http://maven.apache.org/xsd/profiles-1.0.0.xsd">
    <profiles>
        <profile>
            <id>netbeans-private</id>
            <activation>
                <property>
                    <name>netbeans.execution</name>
                    <value>true</value>
                </property>
            </activation>
            <properties>
                <netbeans.deployment.server.id>jboss-deployer:localhost:8080#default&amp;/home/dconnolly/jboss-4.2.2.GA</netbeans.deployment.server.id>
            </properties>
        </profile>

  <!--
  see also src/main/filters
  cribbed from
  http://jee-bpel-soa.blogspot.com/2009/02/maven-profiles-filters-resources-and.html
  -->
        <profile>
            <id>test</id>
            <properties>
                <env>test</env>
            </properties>
        </profile>


        <profile>
            <id>dev</id>
            <properties>
                <env>dev</env>
            </properties>
        </profile>

    </profiles>
</profilesXml>